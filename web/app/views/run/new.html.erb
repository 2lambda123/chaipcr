<div id="maincontent">
<%= form_for @run, url: run_index_path(@run) do |f| %>
	<h2>Do you want to collect real-time PCR data?</h2>
	<div class="radio-button"><%= f.radio_button :qpcr, true, :checked => true %>Yes</div> <div class="radio-button"><%= f.radio_button :qpcr, false %>No</div>
	<div class="submitctl">
	<%= submit_tag "Continue" %>
	</div>
<% end %>
</div>

